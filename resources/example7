    вывод("a");
иначе
    функция1(1, идент);


цикл(;ЛОЖЬ;)
    вывод("2");


пока(2+2)
{
    вывод("2");
    вывод("2");
    вывод("2");
    вывод("2");
    вывод("2");
    вывод("2");
}


целый ф[6] = {1,2,3,4,5,6};
